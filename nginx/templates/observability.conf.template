server {
    listen 80;
    server_name grafana.${DOMAIN};

    location / {
        proxy_pass http://grafana:3000;
        proxy_set_header Host grafana.${DOMAIN};
    }
}

server {
    listen 80;
    server_name kibana.${DOMAIN};

    location / {
        proxy_pass http://kibana:5601;
        proxy_set_header Host kibana.${DOMAIN};
    }
}